      <div class="page-container page-wrapper">
        {% if title %}
          <section>
            <div class="section-label {% if largeTitle %}section-label--header {% endif %}accent-border">
              <h1 class="section-title">{{ title }}</h1>
            </div>

            {% if description and hideDescription != true %}
              <div class="card accent-border">
                <p class="header-description">{{ description }}</p>
              </div>
            {% endif %}
          </section>
        {% endif %}

        {% block content %}
        {% endblock content %}
        
        {% if addEmailSignup != false %}
          {%- include 'partials/newsletter-signup.njk' -%}
        {% endif %}
      </div>
    </div>

