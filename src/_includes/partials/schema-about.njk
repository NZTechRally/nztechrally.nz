<script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "Organization",
    "url": "{{ site.url }}/about",
    "name": "{{ site.title }}",
    "description": "{{ site.description }}",
    "email": "{{ site.email }}",
    "logo": {
      "@type": "ImageObject",
      "url": "{{ site.logo }}",
      "description": "{{ site.logoAlt }}"
    },
    "image": [
      {
        "@type": "ImageObject",
        "url": "{{ site.iamge }}",
        "description": "{{ site.imageAlt }}",
        "width": "{{ site.imageWidth }}",
        "height": "{{ site.imageHeight }}"
      }
    ],
    "event": {
      "@type": "Event",
      "name": "{{ site.title }} 2023",
      "url": "{{ site.url }}",
      "startDate": "2023-07-07T08:00",
      "endDate": "2023-07-07T17:00"
    }
  }
</script>